{
  "name": "jw-library-linker",
  "version": "0.11.3",
  "description": "An Obsidian plugin that helps creating JW Library links",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "changeset version && node version-bump.mjs",
    "release": "VERSION=$(jq -r '.version' manifest.json) && git tag -a $VERSION -m \"Release $VERSION\" && git push origin $VERSION",
    "test:lint": "eslint",
    "test:lint-fix": "eslint --fix",
    "test:types": "tsc --noEmit -skipLibCheck",
    "test:jest": "jest",
    "test:jest-watch": "jest --watch",
    "test": "npm run test:lint && npm run test:types && npm run test:jest",
    "changeset": "npx changeset"
  },
  "keywords": [],
  "author": {
    "name": "Martin Sakowski",
    "url": "https://msakowski.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@swc/core": "^1.15.11",
    "@swc/jest": "^0.2.39",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.1.0",
    "@types/qs": "^6.14.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "builtin-modules": "^5.0.0",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "glob": "^13.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-util": "^30.2.0",
    "jiti": "^2.6.1",
    "js-yaml": "^4.1.1",
    "obsidian": "^1.11.4",
    "prettier": "^3.8.1",
    "qs": "^6.14.1",
    "ts-jest": "^29.4.6",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  },
  "packageManager": "pnpm@10.27.0+sha512.72d699da16b1179c14ba9e64dc71c9a40988cbdc65c264cb0e489db7de917f20dcf4d64d8723625f2969ba52d4b7e2a1170682d9ac2a5dcaeaab732b7e16f04a"
}
